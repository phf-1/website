(use-modules
 (ice-9 match)
 (srfi srfi-1)
 (website conf)
 (website server))

;;;;;;;;;;;;;;;
;; Interface ;;
;;;;;;;;;;;;;;;

(define conf (Conf#mk #f))

(Server#mk (Conf#env conf)
           (Conf#ip conf)
           (Conf#port conf)
           (Conf#content conf)
           (Conf#layout conf)           
           (Conf#login conf)
           (Conf#password conf))
